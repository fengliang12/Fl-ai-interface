  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "prod": "webpack --mode production",
    "analyze": "webpack --mode production --env analyze",
    "lint:fix": "tsc --noEmit & eslint ./src --ext .ts,.tsx,.js --fix",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:staged": "lint-staged",
    "commit": "cz",
    "build:packages": "lerna run build",
    "clean:packages": "lerna clean --yes",
    "publish:packages": "lerna publish",
    "publish:direct": "node scripts/direct-publish.js",
    "test:unit": "jest",
    "test:unit:ci": "jest --coverage --watchAll=false --ci --passWithNoTests --testPathIgnorePatterns=tests/unit/components/AddRedPacket.test.jsx --testPathIgnorePatterns=tests/unit/components/SendRedPacket.test.jsx --testPathIgnorePatterns=tests/unit/components/Dashboard.test.jsx --testPathIgnorePatterns=tests/unit/hooks/useWallet.test.js --testPathIgnorePatterns=tests/unit/contexts/WalletContext.test.jsx",
    "test:e2e": "cypress open",
    "test:e2e:ci": "cypress run --headless --browser chrome",
    "prepare": "husky",
    "ci": "git-cz"
  },
  "jest-stare": {
    "resultDir": "results/jest-stare",
    "reportTitle": "jest-stare!",
    "additionalResultsProcessors": [
      "jest-junit"
    ],
    "coverageLink": "../../coverage/lcov-report/index.html",
    "jestStareConfigJson": "jest-stare.json",
    "jestGlobalConfigJson": "globalStuff.json"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "!(tests)/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,md,json}": [
      "prettier --write"
    ]
  },
